{
    "name": "larapkg/laravel-social-user",
    "description": "Social controller setup for using with Laravel Socialite",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Larapkg\\LaravelSocialUser\\": "src/",
            "Larapkg\\LaravelSocialUser\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Ron Appleton",
            "email": "ron.appleton@mercatura.co.uk"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/socialite": "^5.6",
        "laravel/framework": "^10",
        "laravel/passport": "^11.8"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "orchestra/testbench": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larapkg\\LaravelSocialUser\\Providers\\LaravelSocialProviderServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
